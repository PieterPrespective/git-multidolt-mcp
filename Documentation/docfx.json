{
  "metadata": [
    {
      "dest": "api", 
      "disableDefaultFilter": false, 
      "filter": "filterConfig.yml",
      "disableGitFeatures": false, 
      "src": [
        {
          "src": "../multidolt-mcp", 
          "files": ["*.csproj"]
        },
        {
          "src": "../multidolt-mcp-testing", 
          "files": ["*.csproj"]
        }
      ]
    }], "build": {
        "content": [
          {
            "files": ["api/**.yml", "api/index.md"]
          }, {
            "files": [
              "tdd/**.md", "tdd/**/toc.yml", "userdocs/**.md", "userdocs/**/toc.yml",
              "docsgen/**.md", "docsgen/**/toc.yml", "toc.yml", "*.md"]
            }], "resource": [
              {
                "files": ["images/**"]
              }, {
                "files": ["codecoverage-web/**"]
              }], "dest": "_site", "globalMetadata": {
                "_appTitle": "Embranch MCP Server Documentation", "_appFooter": "Copyright Â© Embranch Team",
                "_enableSearch": true
              }, "template": ["default", "modern"], "postProcessors": ["ExtractSearchIndex"],
              "markdownEngineName": "markdig", "markdownEngineProperties": {
                "markdigExtensions": [
                  "definitionlists", "footnotes", "tasklists", "pipetables", "diagrams"],
                  "enableSourceMap": false
                }
              }
            }